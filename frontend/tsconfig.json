{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "declaration": false,
    "noEmit": false,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./src",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": false,
    "types": [
      "node"
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "paths": {
      "@/*": [
        "*"
      ],
      "*": [
        "*",
        "*.js"
      ],
      "shared/*": ["../../shared/*"]
    }
  },
  "include": [
    "src",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.vue",
    "../shared"
, "src/store/store.ts", "src/router.ts"  ],
  "exclude": [
    "node_modules",
  ],
}